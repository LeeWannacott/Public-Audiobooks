{
  "expo": {
    "name": "libriaudire",
    "slug": "libriaudire",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LibriAudireIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LibriAudireIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.leewannacott.libriaudire"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
