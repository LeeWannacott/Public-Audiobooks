{
  "expo": {
    "name": "Public Audiobooks",
    "slug": "PublicAudiobooks",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/PublicAudiobooksIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/PublicAudiobooksSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/PublicAudiobooksIcon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.leewannacott.libriaudire"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
